#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Data/dev/2022/minimal-blog/node_modules/.pnpm/terser@5.12.0/node_modules/terser/bin/node_modules:/Volumes/Data/dev/2022/minimal-blog/node_modules/.pnpm/terser@5.12.0/node_modules/terser/node_modules:/Volumes/Data/dev/2022/minimal-blog/node_modules/.pnpm/terser@5.12.0/node_modules:/Volumes/Data/dev/2022/minimal-blog/node_modules/.pnpm/node_modules:/Volumes/Data/dev/2022/minimal-blog/node_modules:/Volumes/Data/dev/2022/node_modules:/Volumes/Data/dev/node_modules:/Volumes/Data/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Data/dev/2022/minimal-blog/node_modules/terser/bin/node_modules:/Volumes/Data/dev/2022/minimal-blog/node_modules/terser/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Data/dev/2022/minimal-blog/node_modules/.pnpm/terser@5.12.0/node_modules/terser/bin/node_modules:/Volumes/Data/dev/2022/minimal-blog/node_modules/.pnpm/terser@5.12.0/node_modules/terser/node_modules:/Volumes/Data/dev/2022/minimal-blog/node_modules/.pnpm/terser@5.12.0/node_modules:/Volumes/Data/dev/2022/minimal-blog/node_modules/.pnpm/node_modules:/Volumes/Data/dev/2022/minimal-blog/node_modules:/Volumes/Data/dev/2022/node_modules:/Volumes/Data/dev/node_modules:/Volumes/Data/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Data/dev/2022/minimal-blog/node_modules/terser/bin/node_modules:/Volumes/Data/dev/2022/minimal-blog/node_modules/terser/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../terser/bin/terser" "$@"
else
  exec node  "$basedir/../terser/bin/terser" "$@"
fi
